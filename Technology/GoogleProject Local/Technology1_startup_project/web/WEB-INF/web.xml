<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">
  <servlet>
    <servlet-name>MyServlet</servlet-name>
    <servlet-class>controller.MyServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/.MyServlet</url-pattern>

    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/.addText</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>EditorServlet</servlet-name>
    <servlet-class>controller.EditorServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>EditorServlet</servlet-name>
    <url-pattern>/.editText</url-pattern>
  </servlet-mapping>


</web-app>
